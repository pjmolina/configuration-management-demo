{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Env": "local",
  "Cors": {
    "AuthorizedOrigins": [
      "http://localhost:4200",
      "http://localhost:5050",
      "http://electron.localhost:81",
      "http://photon.localhost:81"
    ]
  },
  "Consul": {
    "Host": "consul",
    "Port": 8500
  },
  "Spa": {
    "photon": {
      "Audience": "https://id.photon.com",
      "ClientId": "photon.local",
      "Brand": "photon"
    },
    "electron": {
      "Audience": "https://id.electron.com",
      "ClientId": "electron.local",
      "Brand": "electron"
    }
  }
}
